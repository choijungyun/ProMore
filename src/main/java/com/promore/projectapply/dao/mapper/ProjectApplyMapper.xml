<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="dto.projectapplyDto">  
	
	
	<resultMap type="com.promore.projectapply.dto.ProjectApplyDto" id="projectapply_dto">
		<result column="apl_no" property="aplNo"/>
		<result column="apl_num" property="aplNum"/>
		<result column="apl_mem_id" property="aplMemId"/>
	</resultMap>
	
<select id="project_apply_list" parameterType="String" resultMap="projectapply_dto">
		select * from project_apply where apl_mem_id=#{aplMemId}
</select>	
	
	
	
	
</mapper>  